{
  "name": "xainik-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postbuild": "node scripts/assert-server-functions.ts",
    "start": "next start -p 3000",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "tsx prisma/seed.ts",
    "preflight": "npm run prisma:generate && npm run spec:check && npm run dead:find && tsc --noEmit && npm test --if-present && npm run webp:check && npm run assert:server-functions || true",
    "spec:check": "tsx scripts/spec-check.ts",
    "dead:find": "tsx scripts/find-dead.ts",
    "webp:check": "tsx scripts/check-webp-policy.ts",
    "assert:server-functions": "tsx scripts/assert-server-functions.ts",
    "smoke:preview": "bash scripts/predeploy-smoke.sh $PREVIEW_URL",
    "smoke:prod": "bash scripts/predeploy-smoke.sh https://xainik.com",
    "self-check": "tsx scripts/self-check.ts",
    "master-runbook": "tsx scripts/master-runbook.ts",
    "captain": "ts-node --transpile-only scripts/build_captain.ts",
    "captain:auto": "node --enable-source-maps --loader ts-node/esm scripts/build_captain.ts --auto",
    "captain:dry": "node --enable-source-maps --loader ts-node/esm scripts/build_captain.ts --dry-run",
    "make:posters": "node scripts/make_posters.mjs",
    "generate:runware": "node scripts/runware-generator.mjs",
    "generate:dalle": "node scripts/dalle-bulk-generator.mjs",
    "generate:supabase": "node scripts/dalle-supabase-generator.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@vercel/og": "^0.8.5",
    "glob": "^11.0.3",
    "lucide-react": "^0.542.0",
    "next": "^14.2.5",
    "next-auth": "^5.0.0-beta.25",
    "node-fetch": "^3.3.2",
    "openai": "^5.19.1",
    "pdf-lib": "^1.17.1",
    "razorpay": "^2.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^3.5.0",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "uuid": "^12.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.14.11",
    "@types/react": "19.1.12",
    "autoprefixer": "^10.4.21",
    "netlify-cli": "^23.5.0",
    "postcss": "^8.5.6",
    "prisma": "^5.18.0",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4",
    "yaml": "^2.5.1"
  }
}